        <filter-name>MobileDeviceDomInspectionFilter</filter-name>
        <url-pattern>*.htm</url-pattern>
    </filter-mapping>
   <filter-mapping>
        <filter-name>MobileDeviceDomInspectionFilter</filter-name>
        <url-pattern>*.htm/*</url-pattern>
    </filter-mapping>
   <filter-mapping>
        <filter-name>MobileDeviceDomInspectionFilter</filter-name>
        <url-pattern>*.html</url-pattern>
    </filter-mapping>
   <filter-mapping>
        <filter-name>MobileDeviceDomInspectionFilter</filter-name>
        <url-pattern>*.html/*</url-pattern>
    </filter-mapping>
   <filter-mapping>
        <filter-name>MobileDeviceDomInspectionFilter</filter-name>
        <url-pattern>*.HTM</url-pattern>
    </filter-mapping>
      <filter-mapping>
        <filter-name>MobileDeviceDomInspectionFilter</filter-name>
        <url-pattern>*.HTM/*</url-pattern>
    </filter-mapping>
   <filter-mapping>
        <filter-name>MobileDeviceDomInspectionFilter</filter-name>
        <url-pattern>*.HTML</url-pattern>
    </filter-mapping>
   <filter-mapping>
        <filter-name>MobileDeviceDomInspectionFilter</filter-name>
        <url-pattern>*.HTML/*</url-pattern>
    </filter-mapping>
